Imports System
Imports System.IO.Disk
Imports Config
Imports BIOSDisk
Imports MinixFS


Module ConfigDefault

    Public Sub Initialize
        BIOSDisk.Initialize
        MinixFS.Initialize
        ' Esses parametros são modificáveis pelo argumento do bootloader
        String.Copy Config.RootFileSystem, "MinixFS"
        String.Copy Config.RootDisk, "BIOSDisk000"
    End
End